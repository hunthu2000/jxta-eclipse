<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="net.jxta.platform.Module"
            class="org.chaupal.jp2p.ui.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.jxta.platform.NetworkManager"
            class="org.chaupal.jp2p.ui.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.jxta.document.Advertisement"
            class="org.chaupal.jp2p.ui.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.jxta.platform.NetworkConfigurator"
            class="org.chaupal.jp2p.ui.property.Jp2pAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="net.osgi.jp2p.component.IJp2pComponent"
            class="org.chaupal.jp2p.ui.property.Jp2pComponentAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.jxse.ui.connectivity"
            name="JP2P Connectivity Monitor"
            parentCategory="org.eclipse.ui">
      </category>
      <view
            allowMultiple="false"
            category="org.eclipse.jxse.ui.connectivity"
            class="org.chaupal.jp2p.ui.context.JxseContainerNavigator"
            id="org.eclipse.jxse.ui.context.view"
            name="JP2P Service Context View"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.jxse.ui.connectivity"
            class="org.chaupal.jp2p.ui.peergroup.PeerGroupNavigator"
            id="org.eclipselabs.jxse.ui.context.peergroups"
            name="JP2P PeerGroup Viewer"
            restorable="true">
      </view>
      <view
            category="org.eclipse.jxse.ui.connectivity"
            class="org.chaupal.jp2p.ui.monitor.ConnectivityViewPart"
            id="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
            name="JP2P Connectivity Monitor">
      </view>
      <view
            allowMultiple="true"
            category="org.eclipse.jxse.ui.connectivity"
            class="org.chaupal.jp2p.ui.view.ModuleTableViewer"
            id="org.eclipse.jxse.ui.view.ModuleTableViewer"
            name="JP2P Module Viewer ">
      </view>
      <view
            class="org.chaupal.jp2p.ui.advertisement.AdvertisementTableViewer"
            id="org.eclipse.jxse.ui.view.AdvertisementTableViewer"
            name="Advertisement Viewer"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.chaupal.jp2p.ui.view.advertisements.AdvertisementNavigator"
            id="org.condast.jxta.ide.view.jxta.advertisements"
            name="JP2P Advertisement Viewer"
            restorable="true">
      </view>
   </extension>
   <extension
         id="org.eclipse.jxse.content"
         name="JXSE Content"
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.chaupal.jp2p.ui.provider.JxseServiceContentProvider"
            id="org.eclipse.jxse.content"
            labelProvider="org.chaupal.jp2p.ui.provider.JxseLabelProvider"
            name="JP2P Context"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.osgi.jp2p.component.IJp2pComponent">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="java.lang.Object">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.chaupal.jp2p.ui.provider.NetworkConfiguratorContentProvider"
            id="org.eclipse.jxse.content.network.configurator"
            labelProvider="org.chaupal.jp2p.ui.provider.JxseLabelProvider"
            name="JP2P Network Configurator Content"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.jxta.platform.NetworkConfigurator">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="java.net.URI">
               </instanceof>
               <instanceof
                     value="java.security.cert.X509Certificate">
               </instanceof>
            </or>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            contentProvider="org.chaupal.jp2p.ui.view.advertisements.JxseAdvertisementContentProvider"
            id="org.chaupal.jp2p.ui.view.advertisements"
            labelProvider="org.chaupal.jp2p.ui.view.advertisements.JxseAdvertisementLabelProvider"
            name="JP2P Advertisement Content"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.osgi.jp2p.component.IJp2pComponent">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="net.jxta.document.Advertisement">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.chaupal.jp2p.ui.peergroup.PeerGroupContentProvider"
            id="org.eclipse.jxse.content.peergroup"
            labelProvider="org.chaupal.jp2p.ui.peergroup.JxsePeerGroupLabelProvider"
            name="JP2P PeerGroup Context"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="net.osgi.jp2p.context.Swarm">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="net.jxta.peergroup.PeerGroup">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
  </extension>
   <extension
         id="org.condast.jxta.ide.view.jxta.container.viewer"
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="org.eclipse.jxse.ui.context.view">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.eclipse.jxse.content">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerContentBinding
            viewerId="org.condast.jxta.ide.view.jxta.advertisements">
         <includes>
            <contentExtension
                  pattern="org.condast.jxse.advertisement">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerContentBinding
            viewerId="org.eclipselabs.jxse.ui.context.peergroups">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.eclipse.jxse.content.peergroup">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
    <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.chaupal.jp2p.ui.perspective.Perspective"
            fixed="false"
            id="org.eclipse.jxse.perspective"
            name="JXSE IDE Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jxse.perspective">
         <view
               closeable="true"
               id="org.eclipse.jxse.ui.context.view"
               minimized="false"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               relationship="bottom"
               relative="org.eclipse.jxse.ui.context.view"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipselabs.jxse.ui.context.peergroups"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.views.PropertySheet"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.console.ConsoleView"
               minimized="false"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="org.eclipse.ui.views.ProblemView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.console.ConsoleView">
         </view>
         <view
               id="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
               minimized="false"
               relationship="right"
               relative="org.eclipse.jxse.ui.context.view"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.jxse.ui.view.ModuleTableViewer"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.jxse.ui.view.AdvertisementTableViewer"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.jxse.ui.monitor.ConnectivityViewPart"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
</plugin>
